{
  "manifest_version": 2,
  "name": "Story Gone - Hide Stories on Facebook™",
  "description": "Story Gone is a simple extension that hides Facebook™ stories. Just install and refresh your feed.",
  "version": "1.2",
  "authors": [ {
    "name": "Syed Husain",
    "website": "https://husainsyed.github.io/*"
   } ],
   
  "content_scripts": [{
    "css": ["inject.css"],
    "matches": ["https://www.facebook.com/*", "https://m.facebook.com/*", "https://touch.facebook.com/*", "https://web.facebook.com/*"]
  }],

  "icons": {
    "16": "images/16 x 16.png",
    "48": "images/48 x 48.png",
    "128": "images/128 x 128.png"
  }

  
 
}